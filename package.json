    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "express-validator": "^6.15.0",
    "glob": "7.1.6",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "node-fetch": "^2.6.9",
    "npm": "^9.6.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "^1.5.0",
